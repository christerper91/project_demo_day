<h1>COMPLETE BOOKING LIST</h1>



<table class= "table table-striped table-hover table-bordered ">
  <thead class="table-dark">
    <tr>
      <th>Created By</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Type Of Car</th>
      <th>Supplier</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

      <% @cars.each do |b| %>
        <tr>
          <td><%= b.user.email %></td>
          <td><%= b.booking.start_date %><br><strong>Pickup Location:</strong><%= b.booking.pickup_location.upcase %></td>
          <td><%= b.booking.end_date %><br><strong>Dropoff Location:</strong><%= b.booking.dropoff_location.upcase %></td>
          <td><%= b.type_of_car.upcase %><br><strong>Plate No:</strong><%= b.car_plate_no.upcase %></td>
          <td><%= b.supplier.company_name %></td>
          <td><%= link_to 'Show Detail', b %></td>
          <td><%= link_to 'Edit', edit_car_path(b) %></td>
          <td><%= link_to 'Destroy', b, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>

  </tbody>
</table>
