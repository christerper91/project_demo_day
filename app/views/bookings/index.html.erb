
<h1>Bookings List</h1>

<table>
  <thead>
    <tr>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Pickup Location</th>
      <th>Dropoff Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

      <% @bookings.each do |b| %>
        <tr>
          <td><%= b.start_date %></td>
          <td><%= b.end_date %></td>
          <td><%= b.pickup_location %></td>
          <td><%= b.dropoff_location %></td>
          <td><%= link_to 'Show', b %></td>
          <td><%= link_to 'Edit', edit_booking_path(b) %></td>
          <td><%= link_to 'Destroy', b, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>

  </tbody>
</table>

<br>

<%= link_to 'New Booking', new_booking_path %>
<%= link_to 'Back', root_path %>
