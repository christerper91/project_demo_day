<h1>Homepage</h1>
<br>
<% if user_signed_in? || admin_signed_in?%>
  <%= link_to "User Log Out",  destroy_user_session_path, method: :delete %>
  <%= link_to "User Profile",  edit_user_registration_path %>
  <br>
  <br>
  <%= link_to "Admin Log Out",  destroy_admin_session_path, method: :delete %>
  <%= link_to "Admin Profile",  edit_admin_registration_path %>



<% else %>
  <%= link_to "User Log In",  new_user_session_path %>
  <%= link_to "User Sign Up",  new_user_registration_path %>
  <br>
  <br>
  <%= link_to "Admin Log In",  new_admin_session_path %>
  <%= link_to "Admin Sign Up",  new_admin_registration_path %>

<% end %>
